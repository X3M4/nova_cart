<odoo>
    <record model="ir.ui.view" id="product_template_custom_fields_form_view_inherit">
        <field name="name">product.template.custom.fields.form</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_form_view"/>
        <field name="arch" type="xml">
            <page name="general_information" position="after">
                <page name="project_fields" string="Project Fields">
                    <group>
                        <field name="objective"/>
                        <field name="unit"/>
                        <field name="time"/>
                        <field name="target"/>
                        <field name="number_of_workers"/>
                        <field name="receptors"/>
                    </group>
                </page>
            </page>
        </field>
    </record>
</odoo>